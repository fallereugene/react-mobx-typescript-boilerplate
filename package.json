    "scripts": {
        "patch": "npm version patch",
        "minor": "npm version minor",
        "major": "npm version major",
        "build": "webpack --config webpack.config.mjs --progress --env mode=production",
        "start": "npm run generate:types && webpack serve --env mode=dev --env isDevServer --config webpack.config.mjs --progress",
        "format": "npm run fix:eslint && npm run fix:prettier && npm run check:eslint",
        "fix:eslint": "eslint --fix --ext .js,.jsx,.ts,.tsx .",
        "fix:prettier": "prettier --write \"**/*.*\" --ignore-unknown",
        "check:eslint": "eslint \"**/*.*\"",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build",
        "commit": "cz",
        "prepare": "husky install",
        "generate:changelog": "auto-changelog -c .changelog/.auto-changelog -t .changelog/changelog-template.hbs --handlebars-setup .changelog/auto-changelog-setup.js",
        "generate:types": "node scripts/generate-types.mjs",
        "version": "npm run generate:changelog -- --package && git add --all",
        "test": "jest --passWithNoTests --config='.tests/jest-tests.config.js'",
        "test:watch": "jest --config='.tests/jest-full.config.js' --watch",
        "test:coverage": "jest --config='.tests/jest-tests.config.js' --coverage",
        "update-caniuse": "npx browserslist@latest --update-db"
    },
    "config": {
        "commitizen": {
            "path": "node_modules/cz-customizable"
        }
    },
    "author": "Faller E.",
    "license": "MIT",
